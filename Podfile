source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'
# don't display any warnings thrown by 3rd party Cocoapods
inhibit_all_warnings!
# in order to use CocoaPods written in Swift, you must explicitly include use_frameworks!
use_frameworks!

target 'SpikeyMcSpikerson' do
    pod 'Alamofire'
end

target 'SpikeyMcSpikersonSpecs' do
    pod 'Quick'
    pod 'Nimble'
end
